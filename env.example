# OncoSwitch Environment Variables
# Copy this file to .env.local and update the values

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=your-google-analytics-id
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your-vercel-analytics-id

# Contact Information
NEXT_PUBLIC_CONTACT_EMAIL=contact@oncoswitch.ai
NEXT_PUBLIC_SUPPORT_EMAIL=support@oncoswitch.ai

# Social Media (Optional)
NEXT_PUBLIC_LINKEDIN_URL=https://linkedin.com/company/oncoswitch
NEXT_PUBLIC_TWITTER_URL=https://twitter.com/oncoswitch
NEXT_PUBLIC_GITHUB_URL=https://github.com/oncoswitch

# Development
NODE_ENV=development
